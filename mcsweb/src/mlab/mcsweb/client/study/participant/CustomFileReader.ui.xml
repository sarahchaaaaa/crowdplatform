<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<!--h1>Text File Reader</h1-->
		<div>
			<span>Select a text/csv file. Each line of the file is an entry, and the format: email,first_name,last_name,organization</span>
			<br/> 
			<br/>
			<input type="file" id="fileInput"/>
		</div>
		<br/>
		<pre id="fileDisplayArea"/>
		<br/>
		<br/>
		<b:Row>
			<b:Column size="MD_3">
				<b:Button text="Submit"></b:Button>
			</b:Column>
			<b:Column size="MD_3">
				<b:Button text="Cancel"></b:Button>
			</b:Column>
		</b:Row>
	</g:HTMLPanel>
</ui:UiBinder> 