<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<h:Br></h:Br>
		<h:Br></h:Br>
		<b:Panel type="SUCCESS">
			<b:PanelHeader>
				<b:Heading size="H3" text="Ping History" />
			</b:PanelHeader>
			<b:PanelBody>
				<g:HTMLPanel>
					<b:Row>
						<b:Column size="MD_5">
							<b:TextBox placeholder="Email" ui:field="textEmail"></b:TextBox>
						</b:Column>
						<b:Column size="MD_5">
							<b:TextBox placeholder="Identifier" ui:field="textIdentifier"></b:TextBox>
						</b:Column>
					</b:Row>
					<h:Br></h:Br>
					<b:Row>
						<b:Column size="MD_4">
							<b:TextBox placeholder="In Last X Days" ui:field="textDays"></b:TextBox>
						</b:Column>
						<b:Column size="MD_4">
							<b:Button ui:field="searchButton">Search</b:Button>
						</b:Column>
					</b:Row>
					<h:Br></h:Br>
					<b:Row>
						<b:Column size="MD_7">
							<g:Label ui:field="errorLabel">Number of days must be a positive
								integer less than 100</g:Label>
						</b:Column>
					</b:Row>
					<h:Br></h:Br>
					<b:Row>
						<!-->b:Column size="MD_1"></b:Column-->
						<b:Column size="MD_12">
							<g:HTMLPanel ui:field="graphPanel">
							</g:HTMLPanel>
						</b:Column>
						<!-->b:Column size="MD_1"></b:Column-->
					</b:Row>
					<h:Br></h:Br><h:Br></h:Br>
					<g:HTMLPanel ui:field="pinglistPanel">
					</g:HTMLPanel>
				</g:HTMLPanel>
			</b:PanelBody>
		</b:Panel>
	</g:HTMLPanel>
</ui:UiBinder> 