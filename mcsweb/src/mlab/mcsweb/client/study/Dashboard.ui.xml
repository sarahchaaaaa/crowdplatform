	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:Row>
		<b:Column size="MD_4">
			<h:Br />
			<h:Br />
			<g:HTMLPanel addStyleNames="col-lg-10">
				<b:Panel>
					<!-->b:PanelHeader>
						<b:Heading size="H4" text="Manage Participation" />
					</b:PanelHeader-->
					<b:LinkedGroup>
						<b:LinkedGroupItem text="Summary" ui:field="summaryLink" />
						<b:LinkedGroupItem text="File Name Mapping" ui:field="fileMapLink" />
						<b:LinkedGroupItem text="Send Notification" ui:field="sendNotificationLink" />
						<b:LinkedGroupItem text="Send Command" ui:field="sendCommandLink" />
						<b:LinkedGroupItem text="Ping Statistics" ui:field="pingStatLink" />
						<b:LinkedGroupItem text="File Object Statistics" ui:field="objectStatLink" />
					</b:LinkedGroup>

				</b:Panel>

			</g:HTMLPanel>

		</b:Column>
		<b:Column size="MD_8">
			<g:HTMLPanel ui:field="contentPanel">

			</g:HTMLPanel>
		</b:Column>
		</b:Row>
