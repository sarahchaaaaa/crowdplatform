<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<h:Br></h:Br>
		<h:Br></h:Br>
		<b:Panel type="SUCCESS">
			<b:PanelHeader>
				<b:Heading size="H3" text="Summary" />
			</b:PanelHeader>
			<b:PanelBody>
				<!--g:HTMLPanel ui:field="listPanel"> <g:Label>Work in progress. This 
					section display summary of the study participation. - Number of participants 
					- Device type distribution (iOS/Android) -OS version - App version</g:Label> 
					</g:HTMLPanel -->
				<b:Column size="MD_1"></b:Column>
				<b:Column size="MD_5">
					<b:Row>
						<b:Heading size="H3" text="Total Participant : "
							ui:field="headingTotal"></b:Heading>
					</b:Row>
					<b:Row>
						<b:Heading size="H4" text="Installed on Devices : "
							ui:field="headingInstalled"></b:Heading>
					</b:Row>
				</b:Column>
				<b:Column size="MD_6">
					<b:Row>
						<g:HTMLPanel ui:field="chartPanel">
						</g:HTMLPanel>
					</b:Row>
				</b:Column>
			</b:PanelBody>
		</b:Panel>
		<h:Br></h:Br>
		<h:Br></h:Br>
		<b:Panel type="PRIMARY">
			<b:PanelHeader>
				<b:Heading size="H3" text="Ping Statistics"></b:Heading>
			</b:PanelHeader>
			<b:PanelBody>
				<b:Row>
					<b:Column size="MD_6">
						<b:Row>
							<g:HTMLPanel ui:field="pingGraphPanel">
							</g:HTMLPanel>
						</b:Row>
						<b:Row>
							<g:HTMLPanel ui:field="uniquePingGraphPanel">
							</g:HTMLPanel>
						</b:Row>
					</b:Column>
					<b:Column size="MD_6">
						<g:HTMLPanel ui:field="pingQueryPanel">
							<b:Row>
								<b:Column size="MD_4">
									<g:Label>No ping in last</g:Label>
								</b:Column>
								<b:Column size="MD_3">
									<b:TextBox ui:field="textboxDaysForPing">7</b:TextBox>
								</b:Column>
								<b:Column size="MD_2">
									<g:Label>days</g:Label>
								</b:Column>
								<b:Column size="MD_3">
									<b:Button ui:field="buttonSearchPing" type="INFO">Refresh</b:Button>
								</b:Column>
							</b:Row>
							<h:Br></h:Br>
							<g:HTMLPanel ui:field="pingListPanel"></g:HTMLPanel>
						</g:HTMLPanel>
					</b:Column>
				</b:Row>
			</b:PanelBody>
		</b:Panel>
		<h:Br></h:Br>
		<h:Br></h:Br>
		<b:Panel type="PRIMARY">
			<b:PanelHeader>
				<b:Heading size="H3" text="Object Statistics"></b:Heading>
			</b:PanelHeader>
			<b:PanelBody>
				<b:Row>
					<b:Column size="MD_6">
						<g:HTMLPanel ui:field="objectGraphPanel">
						</g:HTMLPanel>
					</b:Column>
					<b:Column size="MD_6">
						<g:HTMLPanel ui:field="objectQueryPanel"></g:HTMLPanel>
						<b:Row>
							<b:Column size="MD_4">
								<g:Label>No data in last</g:Label>
							</b:Column>
							<b:Column size="MD_2">
								<b:TextBox ui:field="textboxDaysForObject">7</b:TextBox>
							</b:Column>
							<b:Column size="MD_2">
								<g:Label>days</g:Label>
							</b:Column>
							<b:Column size="MD_3">
								<b:Button ui:field="buttonSearchObject" type="INFO">Refresh</b:Button>
							</b:Column>
						</b:Row>
						<h:Br></h:Br>
						<g:HTMLPanel ui:field="objectListPanel"></g:HTMLPanel>
					</b:Column>
				</b:Row>
			</b:PanelBody>
		</b:Panel>
		<h:Br></h:Br>
		<h:Br></h:Br>
		<b:Panel type="PRIMARY">
			<b:PanelHeader>
				<b:Heading size="H3" text="Labeling Statistics"></b:Heading>
			</b:PanelHeader>
			<b:PanelBody>
				<b:Row>
					<b:Column size="MD_6">
						<g:HTMLPanel ui:field="labelGraphPanel">
						</g:HTMLPanel>
					</b:Column>
					<b:Column size="MD_6">
						<g:HTMLPanel ui:field="labelQueryPanel"></g:HTMLPanel>
						<b:Row>
							<b:Column size="MD_4">
								<g:Label>No Label in last</g:Label>
							</b:Column>
							<b:Column size="MD_2">
								<b:TextBox ui:field="textboxDaysForLabel">7</b:TextBox>
							</b:Column>
							<b:Column size="MD_2">
								<g:Label>days</g:Label>
							</b:Column>
							<b:Column size="MD_3">
								<b:Button ui:field="buttonSearchLabel" type="INFO">Refresh</b:Button>
							</b:Column>
						</b:Row>
						<h:Br></h:Br>
						<g:HTMLPanel ui:field="labelListPanel"></g:HTMLPanel>
					</b:Column>
				</b:Row>
			</b:PanelBody>
		</b:Panel>
		<h:Br></h:Br>
		<h:Br></h:Br>
		<h:Br></h:Br>
		<h:Br></h:Br>
	</g:HTMLPanel>
</ui:UiBinder> 