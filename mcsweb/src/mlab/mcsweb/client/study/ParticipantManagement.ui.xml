				<b:Panel>
					<!--b:PanelHeader>
						<b:Heading size="H4" text="Manage Participation" />
					</b:PanelHeader-->
					<b:LinkedGroup>
						<b:LinkedGroupItem text="All" ui:field="allParticipantLink" active="true"/>
