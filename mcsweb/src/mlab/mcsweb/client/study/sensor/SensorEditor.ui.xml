<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:Row>
			<b:Column size="MD_2">
				<b:Button pull="RIGHT" text="Back" ui:field="backButton"></b:Button>
			</b:Column>
			<b:Column size="MD_7">

			</b:Column>
			<b:Column size="MD_3">
				<b:Button pull="LEFT" text="Save" ui:field="saveButton"></b:Button>
				<b:Button text="Publish" ui:field="publishButton"></b:Button>
			</b:Column>
		</b:Row>
		<h:Br></h:Br>
		<b:Row>
			<b:Column size="MD_6"></b:Column>
			<b:Column size="MD_6">
				<b:Heading size="H4" text="Test" ui:field="editorError"></b:Heading>
			</b:Column>
			
		</b:Row>		
		<b:Row>
			<b:Column size="MD_2">
			</b:Column>
			<b:Column size="MD_2">
				<h:Br></h:Br>
				<h:Br></h:Br>
				<b:Panel>
					<!--b:PanelHeader>
						<b:Heading size="H4" text="Manage Participation" />
					</b:PanelHeader/-->
					<b:LinkedGroup>
						<b:LinkedGroupItem text="Form" ui:field="formLink"/>
						<b:LinkedGroupItem text="Trigger-Action" ui:field="triggerLink"/>
					</b:LinkedGroup>
				</b:Panel>

			</b:Column>
			<b:Column size="MD_1">
			</b:Column>
			<b:Column size="MD_6">
				<g:HTMLPanel ui:field="contentPanel"></g:HTMLPanel>
			</b:Column>
			<b:Column size="MD_1">
			</b:Column>
		</b:Row>
		<h:Br></h:Br>
		<h:Br></h:Br>
		<h:Br></h:Br>


	</g:HTMLPanel>
</ui:UiBinder> 