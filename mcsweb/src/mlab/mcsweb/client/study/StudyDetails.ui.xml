<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.center{
		text-align:ceneter;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:Row addStyleNames="col-lg-offset-1 col-md-offset-1 col-sm-offset-1 col-xs-offset-1">
			<b:AnchorButton type="LINK" ui:field="homeAnchor">Home</b:AnchorButton>
		</b:Row>
		<b:Row addStyleNames="col-lg-offset-3 col-md-offset-3 col-sm-offset-3 col-xs-offset-2">
			<b:Heading size="H3" ui:field="nameHeading" text="Nethealth" addStyleNames="{style.center}"></b:Heading>
		</b:Row>
		<b:Row addStyleNames="col-lg-offset-2 col-md-offset-2 col-sm-offset-2 col-xs-offset-2 col-lg-8 col-md-8 col-sm-8 col-xs-8">
			<g:HTMLPanel ui:field="detailsContentPanel"></g:HTMLPanel>
		</b:Row>
				
	</g:HTMLPanel>
</ui:UiBinder> 