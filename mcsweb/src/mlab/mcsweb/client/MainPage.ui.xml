<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html" >
	
	<ui:style>
	
	.important {
		font-weight: bold;
	}
	
	.navbar-custom {
    	background-color: #D4F1EC;
	}
	
	.navbar-center{
    	position:absolute;
    	left:70%;
	}
	.panel-border{
		border: 1px solid #BBBBBB;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:Navbar addStyleNames="{style.navbar-custom}">
			<b:NavbarHeader addStyleNames="col-lg-offset-1 col-md-offset-1">
				<b:NavbarBrand>MLab CrowdSensing Platform</b:NavbarBrand>
				<b:NavbarCollapseButton dataTarget="#navbar-collapse"/>
			</b:NavbarHeader>
			<b:NavbarCollapse b:id="navbar-collapse">
				<b:NavbarNav pull="RIGHT">
					<!--b:AnchorListItem active="true" ui:field="globalHomeAnchor">Home</b:AnchorListItem-->
					<b:AnchorListItem ui:field="logoutAnchor" >Logout</b:AnchorListItem>
				</b:NavbarNav>
			</b:NavbarCollapse>
		</b:Navbar>
		<g:HTMLPanel ui:field="mainPageContentPanel"></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 