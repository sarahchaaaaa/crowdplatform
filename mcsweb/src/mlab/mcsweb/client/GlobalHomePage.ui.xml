<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.important {
		font-weight: bold;
	}
	
	.navbar-custom {
    	background-color: #0D1F36;
	}
	
	.navbar-center{
    	position:absolute;
    	left:70%;
	}
	
	
	
	</ui:style>
	<g:HTMLPanel>
		<b:Navbar addStyleNames="{style.navbar-custom}">
			<b:NavbarHeader addStyleNames="col-lg-offset-1 col-md-offset-1">
				<b:NavbarBrand ui:field="koiosBrand" color="white">Koios</b:NavbarBrand>
				<b:NavbarCollapseButton dataTarget="#navbar-collapse"/>
			</b:NavbarHeader>
			<b:NavbarCollapse b:id="navbar-collapse" addStyleNames="{style.navbar-center}">
				<!--b:NavbarNav >
					<b:AnchorListItem active="true" ui:field="globalHomeAnchor">Home</b:AnchorListItem>
					<b:AnchorListItem ui:field="loginAnchor" active="true">Login</b:AnchorListItem>
				</b:NavbarNav-->
			</b:NavbarCollapse>
		</b:Navbar>
		<g:HTMLPanel ui:field="globalHomeContentPanel"></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 